<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>{% block title %}Bienvenidos - TiendaKS!{% endblock %}</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="{{ asset('css/tiendaks/general/componentes/navbar/navbar.css') }}">
    
    {% block stylesheets %}
      {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
      {{ encore_entry_script_tags('app') }}
    {% endblock %}

    {% block css %}{% endblock %}
  </head>
  <body>
    {{ component('General:NavbarComponent', {
      usuario: {
        'logged': usuarioLogeado.logged,
        'valor': usuarioLogeado.valor
      },
      subnavbar: {
        'frente': {
          'nombre': 'Inicio',
          'enlace': {
            'tipo': 'ruta',
            'valor': 'app_frontend_inicio_mostrar_inicio'
          }
        },
        'contenido': {
          'valores': configuracionEntorno.categorias,
          'especificiones': {
            'listable': false,
            'atributo': 'ct_nombre'
          }
        },
        'auxiliar': {
          'nombre': 'Vender',
          'enlace': {
            'tipo': 'url',
            'valor': '#'
          }
        },
        'final': {
          'nombre': 'Ver más',
          'enlace': {
            'tipo': 'url',
            'valor': '#'
          }
        },
        'resposive': {
          'frente': {
            'nombre': 'Inicio',
            'enlace': {
              'tipo': 'ruta',
              'valor': 'app_frontend_inicio_mostrar_inicio'
            }
          },
          'sucundario': {
            'nombre': 'Mi cuenta',
            'enlace': {
              'tipo': 'url',
              'valor': '#'
            }
          },
          'contenido': {
            'nombre': 'Categorias',
            'enlace': {
              'tipo': 'url',
              'valor': '#'
            }
          },
          'auxiliar': {
            'nombre': 'Vender',
            'enlace': {
              'tipo': 'url',
              'valor': '#'
            }
          },
          'final': {
            'nombre': 'Ofertas',
            'enlace': {
              'tipo': 'url',
              'valor': '#'
            }
          }
        }
      },
    }) }}
    {% block body %}{% endblock %}
    {% block js %}{% endblock %}
  </body>
</html>
